<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- APP를 배포할 명칭 -->
    <string name="fsp_appName">울산과학대학교</string>
    <!--  서버 접속하는 초기 url -->
    <string name="fsp_startUrl">http://m.uc.ac.kr/WebJSON?fsp_action=StartAction</string>
    <!--  현재 app의 버전 -->
    <string name="fsp_appVersion">1.3.0</string>
    <!--  HTTP Connection Timeout : 1000 -> 1 초 임. -->
    <string name="fsp_httpTimeout">60</string>
    <!--  HTTP Read Timeout : 1000 -> 1 초 임. -->
    <string name="fsp_httpReadTimeout">60</string>
    <!--  HTTP Encoding -->
    <string name="fsp_httpEncoding">utf-8</string>
    <!-- 데이터베이스 버전 -->
    <string name="fsp_dbVersion">1</string>
    
    <!-- 시작 메인 페이지 -->
    <string name="fsp_startMenuUrl">ac.uc.mobile.pns.WebMainActivity</string>
    
    <!-- 디바이스 신청 페이지 주소 https://app.seoultech.ac.kr-->
    <string name="fsp_deviceReqUrl"><![CDATA[http://m.uc.ac.kr/WebJSON?fsp_action=DeviceRequestAction&fsp_cmd=request]]></string>
    
    <!-- 디바이스 신청 상태 페이지 주소 -->
    <string name="fsp_deviceReqStatusUrl"><![CDATA[http://m.uc.ac.kr/WebJSON?fsp_action=DeviceRequestAction&fsp_cmd=select]]></string>
    
    <!-- 푸시 서비스 사용 여부 -->
    <string name="fsp_usePush">true</string>

    <!-- 푸시 버전  -->
    <string name="fsp_pushVersion">F</string>

    <!-- 구글 개발자 아이디 -->
    <string name="fsp_c2dmID">550423056836</string>
    
    <!-- 최초로 APP 설치 후 디바이스에 대한 인증 타입 : U - 사용자 아이디  -->
    <string name="fsp_deviceAuthType">A</string>
    
    <!-- config 처리 클래스명  -->
    <string name="fsp_myConfigName">ac.uc.mobile.pns.PNSAppConfig</string>
    
    <!-- fsp use menubar  -->
    <string name="fsp_useMenubar">false</string>
    
    <!-- fsp use titlebar  -->
    <string name="fsp_useTitlebar">false</string>
    
    <!--  기본 로그 레벨을 지정한다. -->
    <string name="fsp_logLevel">debug=y;info=y;warn=y;error=y;fatal=y;js=y;ex=y;verbose=y;</string>
    
    <!-- 강제로 추가할 js 파일의 경로 설정 -->
	<string-array name="fsp_includeScriptFiles">
	</string-array>
	
	<!-- load 가 완료된 후에 호출해줘야 하는 자바스크립트 함수 -->
	<string name="fsp_loadScriptName"></string>
	
	<!-- 푸시 메시지를 관리할 클래스  -->
    <string name="fsp_pushManagerClassName"></string>	
    
    <!-- 기기 관리자 사용 여부  -->
 	<string name="fsp_deviceAdmin">false</string>
    
</resources>
