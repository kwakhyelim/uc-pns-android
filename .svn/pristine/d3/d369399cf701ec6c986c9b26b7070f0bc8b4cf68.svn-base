<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="fill_parent"
    android:layout_height="fill_parent"  
    android:orientation="vertical"
    style="@style/FSPTheme.list" 
    xmlns:android="http://schemas.android.com/apk/res/android">
 
 	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="0dip"
		android:orientation="vertical"
		android:layout_weight="1">
        <LinearLayout 
        	android:layout_width="fill_parent" 
        	android:layout_height="21dip"
        	android:gravity="center_vertical" 
        	android:orientation="vertical" 
        	style="@style/FSPTheme.list.headerBg">
        	
            <TextView  
		    	android:id="@+id/txtGrpNm" 
		    	android:layout_height="wrap_content" 
		    	android:layout_marginLeft="10dip" 
		    	style="@style/FSPTheme.list.headerName"
		    	android:layout_width="wrap_content"
		    	android:text="@string/fsp_title_setting_lock">
		    </TextView>
        </LinearLayout>		
         
	    <LinearLayout  
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        android:paddingLeft="10dip"
	        android:paddingRight="10dip"
	        android:background="@drawable/fsp_list_row_62_n">
	        <TextView
	            style="@style/FSPTheme.lb"
	            android:layout_weight="1"   
	            android:text="@string/fsp_setting_lb_lockyn" />	
	        <CheckBox
	            android:id="@+id/chkLock"
	            style="@style/FSPTheme.chk" />
	    </LinearLayout>        
	    
		<!--  2번째 시간 입력 부분  -->
	    <LinearLayout
	        android:layout_width="fill_parent"        
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        android:paddingLeft="10dip"
	        android:paddingRight="10dip"
	        android:background="@drawable/fsp_list_row_62_n"
	        android:gravity="center_vertical">
			<TextView
		        style="@style/FSPTheme.lb"    
		        android:layout_weight="1"
		        android:text="@string/fsp_setting_lb_locktime" 
		        android:layout_marginRight="10dip" />			
			 <ImageButton
		        android:id="@+id/btnMinUp"        
		        android:layout_width="wrap_content"       
		        android:layout_height="wrap_content"		        
		        android:layout_gravity="center_vertical|right"
		        android:background="@drawable/fsp_btn_plus_n"/> 
			 <EditText
		        android:id="@+id/txtMin"        
		        android:layout_width="35dip"       
		        android:layout_height="25dip"
		        android:gravity="center"
		        android:text="00" 
		        style="@style/FSPTheme.txt"
		        android:layout_marginLeft="1dip"
		        android:layout_marginRight="1dip"
		        android:maxLength="2"
		        android:inputType="number"
		       />  
		       
		        <ImageButton
		        android:id="@+id/btnMinDown"    
		        android:layout_width="wrap_content"     
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical|right"
		        android:background="@drawable/fsp_btn_minus_n"/>
		        <TextView
		        style="@style/FSPTheme.lb"
		        android:text="@string/fsp_setting_lb_lockmin"
		        android:layout_marginLeft="5dip"
		        android:layout_marginRight="5dip"/>
		        
		        <ImageButton
		        android:id="@+id/btnSecUp"
		        android:layout_width="wrap_content"        
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical|right"
		        android:background="@drawable/fsp_btn_plus_n"/>
		        <EditText
			        android:id="@+id/txtSec"
			        style="@style/FSPTheme.txt"
			        android:layout_width="35dip"
			        android:layout_height="25dip"
			        android:gravity="center"
			        android:text="00"
			        android:maxLength="2"
			        android:layout_marginLeft="1dip"
		       		android:layout_marginRight="1dip"
		       		android:inputType="number"
		       		/>
		        <ImageButton
			        android:layout_width="wrap_content"     
			        android:layout_height="wrap_content"
			        android:layout_gravity="center_vertical|right"
			        android:background="@drawable/fsp_btn_minus_n" 
			        android:id="@+id/btnSecDown"/>  
		     <TextView
				android:layout_width="wrap_content"        
				android:layout_height="wrap_content"
				android:text="@string/fsp_setting_lb_locksec"
				style="@style/FSPTheme.lb"
				android:layout_gravity="center_vertical"
				android:layout_marginLeft="5dip"/>	 
	    </LinearLayout>
	    
	    <LinearLayout
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        android:paddingLeft="10dip"
	        android:paddingRight="10dip"
	        android:background="@drawable/fsp_list_row_62_n">
		     <TextView
		        style="@style/FSPTheme.lb" 
		        android:text="@string/fsp_setting_lb_lockpwd"/>
		     <EditText
		        android:id="@+id/edPwd"
		        style="@style/FSPTheme.txt"
		        android:layout_height="wrap_content"
	        	android:layout_width="wrap_content"
		        android:layout_weight="1"
		        android:layout_marginLeft="5dip" 
		        android:paddingLeft="2dip"
		        android:paddingRight="2dip"
		        android:password="true"/>       
	    </LinearLayout>
	    
	    <LinearLayout
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        android:paddingLeft="10dip"
	        android:paddingRight="10dip"
	        android:background="@drawable/fsp_list_row_82_n">
	        <TextView
	        	style="@style/FSPTheme.lb" 
		        android:text="@string/fsp_setting_lb_lockpwd2"/>
		        
	    <EditText
	        android:id="@+id/edPwdConfirm"      
	        style="@style/FSPTheme.txt"
	        android:layout_height="wrap_content"
	        android:layout_width="wrap_content"
	        android:layout_weight="1"    
	        android:layout_marginLeft="5dip" 
	       	android:paddingLeft="2dip"
		    android:paddingRight="2dip"
	        android:password="true"/>  
	    </LinearLayout>   
	</LinearLayout>
	
	<!--  메뉴 바 -->
	<com.sz.fspmobile.view.MenuBarView 
		android:layout_width="fill_parent" 
		android:id="@+id/menuBar"
		android:layout_height="53dip"/>          
</LinearLayout>
