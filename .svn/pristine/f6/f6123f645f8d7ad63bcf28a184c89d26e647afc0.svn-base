<?xml version="1.0" encoding="utf-8"?>
 <LinearLayout
 	xmlns:android="http://schemas.android.com/apk/res/android"
 	android:id="@+id/mainWin"
  	android:layout_width="fill_parent"
  	android:layout_height="fill_parent"
  	android:orientation="vertical">
 	<!--  타이틀 바 -->
	<com.sz.fspmobile.view.TitleBarView 
		android:layout_width="fill_parent" 
		android:id="@+id/titleBar"
		android:visibility="gone"
		android:layout_height="35dip"/>
	 	
   <!-- 윗부분의 페이지를 나타낼 부분  -->
   <!-- 플리퍼로 화면을 감싼다 원래 4   -->
      <LinearLayout 
      	android:layout_width="fill_parent" 
      	android:background="@color/fsp_transparent"
      	android:layout_height="0dip" 
      	android:layout_marginLeft="10dip"
		android:layout_marginRight="10dip"
		android:orientation="vertical"
      	android:layout_weight="2"
      	android:visibility="visible">
      	<!--  윗 부분의 테두리 레이아웃 : 사이즈 결정 필요  -->
   		<LinearLayout 
    		android:layout_height="55dip" 
    		android:layout_width="fill_parent"
    		android:background="@drawable/fsp_menu_top_bg"
    		android:gravity="right">	
			      <RelativeLayout 
			      	android:id="@+id/topMonthLayout"
			      	android:layout_height="fill_parent" 
			      	android:orientation="vertical"
			      	android:layout_marginTop="5dip"
			      	android:layout_marginRight="10dip" android:layout_width="80dip">
			      <TextView 
			      	android:id="@+id/topMonthDate"
			      	android:layout_height="wrap_content"
			      	style="@style/FSPTheme.menuTop.Date"
			      	android:layout_width="fill_parent"
			      	android:gravity="center"
			      	android:text="11월10일"/>
			      <ImageView 
			      	android:id="@+id/topLine"
			      	android:layout_height="wrap_content"
			      	android:background="@drawable/fsp_menu_top_line"
			      	android:layout_marginTop="0dip" 
			      	android:layout_width="fill_parent" 
			      	android:layout_below="@id/topMonthDate"
			      	android:gravity="center"/>
			      <TextView 
			      	android:id="@+id/topDay"
			      	android:layout_width="fill_parent"
			      	android:layout_height="wrap_content"
			      	android:gravity="center"
			      	android:paddingTop="1dip"
			      	style="@style/FSPTheme.menuTop.month"
			      	android:layout_below="@id/topLine"
			      />
			     </RelativeLayout> 	
      	</LinearLayout>
   </LinearLayout>
      
      <!--  7  -->
      <LinearLayout 
      	android:layout_width="fill_parent" 
      	android:layout_height="0dp" 
      	android:layout_weight="7">
          <ViewFlipper 
          	android:layout_height="fill_parent" 
          	android:layout_width="fill_parent"
          	android:id="@+id/menuFlipper" 
          	android:layout_marginBottom="0dip" 
          	android:layout_marginTop="0dip">
          </ViewFlipper>
      </LinearLayout>
      
	<!--  page 건수 처리 부분  -->
	<LinearLayout android:id="@+id/pagingLayout" 
		android:gravity="center" 
		android:layout_weight="1"
		android:layout_width="fill_parent" 
		android:layout_height="wrap_content">
	</LinearLayout>
	
	<!--  메뉴 바 -->
	<com.sz.fspmobile.view.MenuBarView 
		android:layout_width="fill_parent" 
		android:id="@+id/menuBar"
		android:layout_height="53dip"/>
 </LinearLayout>