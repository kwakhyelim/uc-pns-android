<?xml version="1.0" encoding="utf-8"?>

<resources>
	<!--  fsp mobile 기본 테마 -->
	<style name="FSPTheme" parent="android:Theme">
    	<item name="android:windowTitleSize">0dip</item>
		<item name="android:windowTitleBackgroundStyle">@style/FSPTheme.FSPTitleBg</item>
		<item name="android:textSize">13sp</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:textAppearance">@style/FSPTheme.TextApperance</item>
	</style>
    
	<!--  메인 화면 타이틀바 parent="@android:style/WindowTitleBackground"-->
	<style name="FSPTheme.FSPTitleBg">										
		<item name="android:background">@drawable/fsp_titlebar_bg</item>
	</style>	
	
	<!--  fsp mobile 관련 다이얼로그  -->
	<style name="FSPTheme.Dialog" parent="android:style/Theme.Dialog">
		<item name="android:windowBackground">@null</item>
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowIsFloating">true</item>
    </style>

	<style name="FSPTheme.menuTop">
	
	</style>
	<!-- 일자 표시되는 부분의 글자  -->
	<style name="FSPTheme.menuTop.month">
		<item name="android:textSize">11sp</item>
		<item name="android:textColor">#000000</item>
		<item name="android:textStyle">bold</item>
	</style>	

	<!-- 일자 표시되는 부분의 글자  -->
	<style name="FSPTheme.menuTop.Date">
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">#000000</item>
		<item name="android:textStyle">bold</item>
	</style>
		
	<!--  푸시 메시지  -->
	<style name="FSPTheme.pushCount">
		<item name="android:textSize">15sp</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:textStyle">bold</item>
	</style>

	<!--  공지사항 조회 메시지 : bg -->
	<style name="FSPTheme.queryNoticeBg">
		<item name="android:background">#e7e7e7</item>
	</style>
	<!--  공지사항 조회 메시지  -->
	<style name="FSPTheme.queryNotice">
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#191919</item>
		<item name="android:textStyle">bold</item>
		<item name="android:text">공지사항을 가져오는 중입니다.</item>
	</style>
		
<!-- 안드로이드 기본 텍스트  -->
	<style name="FSPTheme.TextApperance" parent="@android:style/TextAppearance">
		<item name="android:textSize">11sp</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:textStyle">bold</item>
	</style>
	
<!-- 상위 기본 버튼 정의 -->
	<style name="btn">
		<item name="android:layout_width">80dp</item>
		<item name="android:textSize">20sp</item>
	</style>
	
	<!-- 버튼  -->
	<style name="btn_close" parent="btn">
		<item name="android:textColor">#000000</item>
		<item name="android:text">@string/fsp_btn_close</item>
	</style>
	<!-- 확인 메시지 창의 cancel버튼  -->
	<style name="btn_confirm_cancel" parent="btn_close">
		<item name="android:text">@string/fsp_btn_confirm_cancel</item>
	</style>
	
	<!-- 확인 메시지 창의 OK 버튼  -->
	<style name="btn_confirm_ok" parent="btn">
		<item name="android:textColor">#ff0000</item>
		<item name="android:text">@string/fsp_btn_confirm_ok</item>
	</style>
	
	<!--  checkbox  -->
	<style name="FSPTheme.chk">
		<item name="android:button">@drawable/fsp_check</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">20dip</item>
	</style>
	
<!-- 메뉴 -->
	<!-- 리스트 백그라운드  -->
	<style name="FSPTheme.list">
		<item name="android:background">#e7e7e7</item>
	</style>
	
	<!-- 그룹 헤더 백그라운드  -->
	<style name="FSPTheme.list.headerBg">
		<item name="android:background">#7c7c7c</item>
		<item name="android:layout_height">21dip</item>
	</style>
	
	<!--  그룹 분류 명 -->
	<style name="FSPTheme.list.headerName">
		<item name="android:background">@color/fsp_transparent</item>
		<item name="android:textSize">10sp</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left|center_vertical</item>
	</style>	
		
	<!-- 메뉴 row 백그라운드  -->
	<style name="FSPTheme.list.menuRowBg">
		<item name="android:background">@drawable/fsp_list_row_82_bg</item>
	</style>	
	
	<!--  메뉴 명  -->
	<style name="FSPTheme.list.menuName">
		<item name="android:background">@color/fsp_transparent</item>
		<item name="android:textSize">13sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
		<item name="android:textColor">#191919</item>
	</style>

	<!--  메뉴 설명  -->
	<style name="FSPTheme.list.menuDesc">
		<item name="android:background">@color/fsp_transparent</item>
		<item name="android:textSize">10.5sp</item>
		<item name="android:textColor">#191919</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
	</style>	

	<!-- 메뉴 row 백그라운드  -->
	<style name="FSPTheme.list.settingRowBg">
		<item name="android:background">@drawable/fsp_list_row_62_bg</item>
		<item name="android:layout_height">41dip</item>
	</style>	
	
	<!--  메뉴 명  -->
	<style name="FSPTheme.list.settingName">
		<item name="android:background">@color/fsp_transparent</item>
		<item name="android:textSize">12sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
		<item name="android:textColor">#191919</item>
	</style>

	<!--  메뉴 설명  -->
	<style name="FSPTheme.list.settingDesc">
		<item name="android:background">@color/fsp_transparent</item>
		<item name="android:textSize">9sp</item>
		<item name="android:textColor">#3c3c3c</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
	</style>
	
	<!-- 메뉴 row 백그라운드  -->
	<style name="FSPTheme.list.pushMsgRowBg">
		<item name="android:background">@drawable/fsp_list_row_82_bg</item>
	</style>	
	
	<!--  메뉴 명  -->
	<style name="FSPTheme.list.pushMsgName">
		<item name="android:background">@color/fsp_transparent</item>
		<item name="android:textSize">13sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
		<item name="android:textColor">#191919</item>
	</style>

	<!--  메뉴 설명  -->
	<style name="FSPTheme.list.pushMsgDesc">
		<item name="android:background">@color/fsp_transparent</item>
		<item name="android:textSize">10.5sp</item>
		<item name="android:textColor">#191919</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
	</style>
	
	<!--  top 메뉴 글자   -->
	<style name="FSPTheme.topMenuTime">
		<item name="android:textSize">10sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
		<item name="android:textColor">@color/fsp_menu_top_list_row_time</item>
	</style>
		
	<!--  top 메뉴 글자   -->
	<style name="FSPTheme.topMenuText">
		<item name="android:textSize">10sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
		<item name="android:textColor">@color/fsp_menu_top_list_row_text</item>
	</style>

<!--  설정 관련  -->
	<!--  메뉴 명  -->
	<style name="FSPTheme.settingName">
		<item name="android:textSize">11sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
		<item name="android:textColor">#191919</item>
	</style>
	<!--  메뉴 설명  -->
	<style name="FSPTheme.settingDesc">
		<item name="android:textSize">9sp</item>
		<item name="android:textColor">#191919</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left</item>
	</style>

	<!--  입력 항목   -->
	<style name="FSPTheme.lb">
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#191919</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left|center_vertical</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
	</style>	
	<style name="FSPTheme.txt">
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#191919</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">left|center_vertical</item>
		<item name="android:background">@drawable/fsp_edt_bg</item>
	</style>	
<!-- 탭 메뉴의 입력 글자  -->
	<style name="FSPTheme.pushMsgBadge">
		<item name="android:textSize">8sp</item>
		<item name="android:textColor">#191919</item>
		<item name="android:textStyle">bold</item>
	</style>
		
<!-- 푸시 메시지 상세 페이지  -->
	<style name="FSPTheme.pushMsg">
		<item name="android:background">#e7e7e7</item>
	</style>

	<!--  흰색, 크기 10, 굵게    -->
	<style name="FSPTheme.white_10_bold">
		<item name="android:textSize">10sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">@color/fsp_white</item>
	</style>

	<!--  흰색, 크기 10, 굵게    -->
	<style name="FSPTheme.gray_12_bold">
		<item name="android:textSize">12sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">@color/fsp_gray1</item>
	</style>	
	
	<!--  흰색, 크기 10, 굵게    -->
	<style name="FSPTheme.gray_13_bold">
		<item name="android:textSize">13.2sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">@color/fsp_gray1</item>
	</style>
							
<!--  레이아웃을 정의 -->
	<!-- 한줄에 하나의 내용만을 꽉 채워 표시하는 속성 -->
	<style name="layout_oneLine">
		<item name="android:gravity">left</item>
		<item name="android:layout_marginBottom">3dp</item>
		<item name="android:layout_marginLeft">6dp</item>
		<item name="android:layout_marginRight">6dp</item>
		<item name="android:layout_marginTop">3dp</item>
		<item name="android:layout_width">fill_parent</item>
	</style>
</resources>
